{
  "name": "more-recipes",
  "version": "1.0.0",
  "private": true,
  "description": "A Recipe management and sharing app",
  "main": "/server/app.js",
  "scripts": {
    "start": "node ./build-server/app.js",
    "start:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "nodemon --watch server --exec babel-node -- server/app.js",
    "dev:client": "webpack --config webpack.dev.js --watch",
    "prod:server": "babel -d ./build-server ./server -s",
    "prod:client": "webpack --config webpack.prod.js",
    "test": "mocha --timeout 500000 --compilers js:babel-core/register server/tests/",
    "coveralls": "nyc npm test && nyc --no-clean npm run test:client && nyc report --reporter=text-lcov | coveralls",
    "test:client": "jest --coverage",
    "heroku-postbuild": "npm run prod:client && npm run prod:server"
  },
  "author": "Lawal Lanre Emmanuel",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "cloudinary": "^1.9.1",
    "compression-webpack-plugin": "^1.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "expect": "^22.0.3",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "moment": "^2.20.1",
    "morgan": "^1.8.2",
    "moxios": "^0.4.0",
    "multer": "^1.3.0",
    "net": "^1.0.2",
    "node-sass": "^4.6.1",
    "nodemailer": "^4.1.0",
    "nodemon": "^1.12.1",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "rc-pagination": "^1.14.0",
    "rc-select": "^7.5.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-google-charts": "^1.5.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.76.0",
    "sequelize": "^4.8.0",
    "sfcookies": "^1.0.2",
    "style-loader": "^0.19.0",
    "toastr": "^2.1.4",
    "webpack": "^3.5.5",
    "webpack-merge": "^4.1.1",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.2.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "enzyme": "^3.2.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.1.0",
    "react-test-renderer": "^16.2.0",
    "should": "^11.2.1",
    "url-loader": "^0.6.2",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "globals": {
      "window": true
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larrystone/BC-26-More-Recipes.git"
  },
